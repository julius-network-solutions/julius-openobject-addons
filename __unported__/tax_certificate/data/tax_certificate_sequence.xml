<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
    <!--
	Sequences types for certificates
	-->
        <record id="seq_type_out_certificate" model="ir.sequence.type">
            <field name="name">Tax Certificate Out</field>
            <field name="code">tax.certificate.out_certificate</field>
        </record>
        <record id="seq_type_in_certificate" model="ir.sequence.type">
            <field name="name">Tax Certificate In</field>
            <field name="code">tax.certificate.in_certificate</field>
        </record>
        
    <!--
	Sequences for certificates
	-->
        <record id="seq_out_certificate" model="ir.sequence">
            <field name="name">Tax Certificate Out</field>
            <field name="code">tax.certificate.out_certificate</field>
            <field eval="3" name="padding"/>
            <field name="prefix">%(year)s/</field>
        </record>
        <record id="seq_in_certificate" model="ir.sequence">
            <field name="name">Tax Certificate In</field>
            <field name="code">tax.certificate.in_certificate</field>
            <field eval="3" name="padding"/>
            <field name="prefix">%(year)s/</field>
        </record>
    </data>
</openerp>
