<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="view_wiki_meeting_report_form">
            <field name="name">blog.post.form</field>
            <field name="model">blog.post</field>
            <field name="inherit_id" ref="website_blog.view_blog_post_form"/>
            <field name="arch" type="xml">
                <group string="Technical" position="after">
                    <group name='content_data' string="Content">
                        <field name="content" nolabel="1" colspan="4" select="1" widget="html"/>
                        <separator colspan="4" string="Meeting"/>
                        <field name="meeting_report" nolabel="1" colspan="4" select="1" widget="html"/>
                    </group>           
                </group>
            </field>
        </record>

    </data>
</openerp>
